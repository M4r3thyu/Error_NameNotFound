<Window x:Class="Error_NameNotFound.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Error_NameNotFound.Views"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Error_NameNotFound"
        mc:Ignorable="d"
        Title="ERROR: Name not Found">
    <Grid MinHeight="450" MinWidth="700">
        <materialDesign:Card>
            <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Height="15" FontSize="10">Datei Bearbeiten Ansicht etc.</TextBlock>
        </materialDesign:Card>
        <materialDesign:Card Margin="0,15,0,0">
            <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Height="17" FontSize="10">Schnellsuchleiste</TextBlock>
        </materialDesign:Card>
        <materialDesign:Card Margin="0,32,0,0" VerticalAlignment="Stretch" MinWidth="100" HorizontalAlignment="Left" Width="101">
        </materialDesign:Card>
        <Button Height="80" Width="70" Margin="10,48,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Name="AND_button" Click="AND_Button_CLicked">
            <Button.Template>
                <ControlTemplate>
                    <Image Source="/Error_NameNotFound;component/Designs/And.png" HorizontalAlignment="Center"></Image>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Height="80" Width="70" Margin="10,138,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Name="OR_button" Click="OR_Button_CLicked">
            <Button.Template>
                <ControlTemplate>
                    <Image Source="/Error_NameNotFound;component/Designs/Or.png" HorizontalAlignment="Center"></Image>
                </ControlTemplate>
            </Button.Template>

        </Button>
        <Canvas Margin="100,32,0,0" Name="Workspace" Background="WhiteSmoke" AllowDrop="True" DragOver="canvas_DragOver" Drop="canvas_Drop">

        </Canvas>
    </Grid>
</Window>
