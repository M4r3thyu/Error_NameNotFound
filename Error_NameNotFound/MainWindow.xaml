<Window x:Class="Error_NameNotFound.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        Title="ERROR: Name not Found">
    <Grid MinHeight="450" MinWidth="700">
        <materialDesign:Card>
            <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Height="15" FontSize="10">Datei Bearbeiten Ansicht etc.</TextBlock>
        </materialDesign:Card>
        <materialDesign:Card Margin="0,15,0,0">
            <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Height="17" FontSize="10">Schnellsuchleiste</TextBlock>
        </materialDesign:Card>
        <materialDesign:Card Margin="0,32,669,0" VerticalAlignment="Stretch" HorizontalAlignment="Left" Width="100" BorderBrush="Black">
            <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Height="17" FontSize="10" Margin="34,185,0,185">Bausteine</TextBlock>
        </materialDesign:Card>
    </Grid>
</Window>
