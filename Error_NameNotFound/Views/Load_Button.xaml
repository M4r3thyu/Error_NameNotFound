<UserControl x:Class="Error_NameNotFound.Views.Load_Button"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Error_NameNotFound.Views"
             xmlns:vm="clr-namespace:Error_NameNotFound.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <vm:Load_Button_vm/>
    </UserControl.DataContext>
    <Grid>
        <Button>
            <Button.InputBindings>
                <MouseBinding Gesture="LeftClick" Command="{Binding ClickCommand}"/>
            </Button.InputBindings>
        </Button>
    </Grid>
</UserControl>