<vm:Logicgatescontrol x:Class="Error_NameNotFound.ViewModel.Oscillator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:Error_NameNotFound.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid>
        <Image Width="100" Height="100" Name="OscillatorUI" Source="/Error_NameNotFound;component/Pictures/Oscillator.png" Grid.ColumnSpan="2"/>
        <TextBlock Name="Intervall" Text="Intervall" Margin="10,78,10,5" TextAlignment="Center"/>
        <TextBox Name="Timeout" Margin="10,38,10,30" TextAlignment="Center" TextChanged="Timeout_TextChanged" MouseEnter="Timeout_MouseEnter" MouseLeave="Timeout_MouseLeave" FontSize="20" />
        <Button Width="10" Height="10" Margin="95,20,-5,70" Name="output1" PreviewMouseLeftButtonDown="Output1_Click" />
        <Button Width="10" Height="10" Margin="95,70,-5,20" Name="output0" PreviewMouseLeftButtonDown="Output0_Click"/>
    </Grid>
</vm:Logicgatescontrol>
