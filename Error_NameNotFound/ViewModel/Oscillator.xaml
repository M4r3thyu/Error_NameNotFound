<vm:Logicgatescontrol x:Class="Error_NameNotFound.ViewModel.Oscillator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:Error_NameNotFound.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid>
        <Image Width="100" Height="100" Name="OscillatorUI" Source="/Error_NameNotFound;component/Pictures/Oscillator.png" Grid.ColumnSpan="2"/>
        <TextBlock Name="Intervall" Text="Intervall:" Grid.ColumnSpan="2" Margin="25,23,25,60" TextAlignment="Center"/>
        <TextBox Name="Timeout" Grid.ColumnSpan="2" Margin="30,40,30,43" TextAlignment="Center" TextChanged="Timeout_TextChanged" MouseEnter="Timeout_MouseEnter" MouseLeave="Timeout_MouseLeave"/>
        <Button Width="10" Height="10" Margin="80,20,10,70" Name="output1" PreviewMouseLeftButtonDown="Output1_Click" Grid.ColumnSpan="2" />
        <Button Width="10" Height="10" Margin="80,70,10,20" Name="output0" PreviewMouseLeftButtonDown="Output0_Click" Grid.ColumnSpan="2"/>
    </Grid>
</vm:Logicgatescontrol>
