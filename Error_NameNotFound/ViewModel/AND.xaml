<vm:Logicgatescontrol x:Class="Error_NameNotFound.AND"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:Error_NameNotFound.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid>
        <Image Width="100" Height="100"   Name="ANDUI" Source="/Error_NameNotFound;component/Pictures/And.png"></Image>
        <Button Width="10" Height="10" Margin="10,20,80,80" Name="input0" PreviewMouseLeftButtonDown="Input0_Click" MouseRightButtonDown="DelConnection_Input0"/>
        <Button Width="10" Height="10" Margin="10,70,80,10" Name="input1" Click="Input1_Click" MouseRightButtonDown="DelConnection_Input1" AllowDrop="true" Drop="StopCableDrag"/>
        <Button Width="10" Height="10" Margin="80,20,10,80" Name="output1" Click="Output1_Click"/>
        <Button Width="10" Height="10" Margin="80,70,10,10" Name="output0" Click="Output0_Click"/>
    </Grid>
</vm:Logicgatescontrol>
