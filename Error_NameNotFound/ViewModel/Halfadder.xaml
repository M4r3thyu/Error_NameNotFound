<vm:Logicgatescontrol x:Class="Error_NameNotFound.ViewModel.Halfadder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:Error_NameNotFound.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">
    <Grid>
        <Image Width="100" Height="100" Name="HalfadderUI" Source="/Error_NameNotFound;component/Pictures/Halfadder.png"></Image>
        <Button Width="10" Height="10" Margin="20,-5,70,95" Name="input0" MouseRightButtonDown="DelConnection_Input0" RenderTransformOrigin="0.875,0.625" AllowDrop="true" Drop="Input0_Drop"/>
        <Button Width="10" Height="10" Margin="70,-5,20,95" Name="input1" MouseRightButtonDown="DelConnection_Input1" AllowDrop="true" Drop="Input1_Drop" />
        <Button Width="10" Height="10" Margin="20,95,70,-5" Name="output1" PreviewMouseLeftButtonDown="Output1_Click"/>
        <Button Width="10" Height="10" Margin="70,95,20,-5" Name="output0" PreviewMouseLeftButtonDown="Output0_Click"/>
    </Grid>
</vm:Logicgatescontrol>
